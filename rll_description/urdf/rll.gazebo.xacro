<?xml version="1.0"?>

<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="rll_gazebo" params="robot_name">

  <!-- Link Table -->    
  <gazebo reference="table">
    <mu1>1.0</mu1>
    <mu2>1.0</mu2>
    <static>true</static>
  </gazebo>
  <gazebo reference="$(arg robot_name)_link_0">
    <static>true</static>
  </gazebo>

  <!-- Link profile_cams -->    
  <gazebo reference="profile_cams">
    <static>true</static>
  </gazebo>

  </xacro:macro>

</robot>

